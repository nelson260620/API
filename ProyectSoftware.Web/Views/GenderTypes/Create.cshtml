@model ProyectSoftware.Web.Data.Entities.GenderType
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_MyLayout.cshtml"; /* forma para que en ves del tener el layout normal tenga el otro */
}


<div class="pagetitle">
    <h1>New GenderTypr</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-asp-controller="Home" asp-action="Dashboard">Home</a></li>
            <li class="breadcrumb-item active"><a asp-action="Index">GenderType</a></li>
            <li class="breadcrumb-item active">GendeType</li>
            <li class="breadcrumb-item active">New GenderType</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section dashboard">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">New GenderType</h5>

            <!-- Floating Labels Form -->
            <form class="row g-3" asp-action="Create" method="POST">
                <div class="col-md-12">
                    <div class="form-floating">
                        <input asp-for="GenderName" class="form-control"  placeholder="@Html.DisplayFor(model => model.GenderName)">
                        <label>@(Html.DisplayNameFor(model => model.GenderName)) GenderName</label>
                        <span asp-validation-for="GenderName" class="Text-Danger"></span>
                    </div>
                </div>
 
                <div class="text-center">
                    <button type="submit" class="btn btn-primary" >Submit</button>
                    <a asp-action="Index" class="btn btn-secondary">return</a>
                </div>
            </form><!-- End floating Labels Form -->

        </div>
    </div>
</section>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}